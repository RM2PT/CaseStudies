	}

	Actor LoanClerk {
		@Description( "The loan clerk is responsible for booking (recording and setting up) the loan") generateLoanLetterAndAgreement
		bookNewLoan
		loanPayment
		closeOutLoan
	}

	Actor Scheduler {
		generateStandardPaymentNotice
		generateLateNotice
	}

